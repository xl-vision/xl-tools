{
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": []
    },
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "semicolon": [true, "never"],
        "trailing-comma": [true, "alaways"],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "member-access": false
    },
    "rulesDirectory": []
}